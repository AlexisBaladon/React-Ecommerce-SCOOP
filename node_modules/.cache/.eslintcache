Detail, amount: number): ItemTicket => {\r\n  let res: ItemTicket;\r\n    \r\n  //item destructuring\r\n  const [id, title, price, pictureUrl]: \r\n  [string, string, number, string] =\r\n  [item.id, item.title, item.price, item.pictureUrl];\r\n\r\n  res = new ItemTicket(id, productDetail.getTicketTitle(title), price, pictureUrl, productDetail, amount);\r\n\r\n  return res;\r\n}\r\n\r\nconst createProductDetail = (category: Category): ProductDetail => {\r\n  let newProductDetail: ProductDetail;\r\n  \r\n  switch(category) {\r\n    case Category.Recipiente:\r\n      newProductDetail = new ProductDetailRecipiente([]);\r\n      break;\r\n    default: \r\n      newProductDetail = new ProductDetailSimple();\r\n      break;\r\n  }\r\n\r\n  return newProductDetail;\r\n}\r\nexport {createTicket, createProductDetail};","F:\\reactApp\\heladeria-e-commerce-baladon\\src\\context\\cartContext.tsx",[],"F:\\reactApp\\heladeria-e-commerce-baladon\\src\\components\\cart\\cartContainer.tsx",[],"F:\\reactApp\\heladeria-e-commerce-baladon\\src\\data\\itemHandler.ts",[],"F:\\reactApp\\heladeria-e-commerce-baladon\\src\\data\\flavorHandler.ts",[],"F:\\reactApp\\heladeria-e-commerce-baladon\\src\\components\\navbar\\cartWidget\\cartWidget.tsx",[],"F:\\reactApp\\heladeria-e-commerce-baladon\\src\\components\\navbar\\accountWidget\\accountWidget.tsx",[],"F:\\reactApp\\heladeria-e-commerce-baladon\\src\\components\\sessions\\login.tsx",["218"],"import React, { useRef, useState } from 'react'\r\nimport { Alert, Button, Card, CloseButton, Form, Modal } from 'react-bootstrap';\r\n\r\nimport './login.css'\r\n\r\ninterface IProps {\r\n  show: boolean;\r\n  onHide: () => any;\r\n  login: (email: string, password: string) => Promise<any>;\r\n  openRegister: () => any;\r\n}\r\n\r\nconst Login: React.FC<IProps> = ({show, onHide, login, openRegister}) => {\r\n  interface IAlert {\r\n    message: string;\r\n    variant: string;\r\n  }\r\n\r\n  const [alertMessage, setAlertMessage] = useState<IAlert | n